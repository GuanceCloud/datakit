
[[inputs.prom]]
    # etcd metrics from http(https)://HOST:PORT/metrics
    # usually modify host and port
    # required
    url = "http://127.0.0.1:2379/metrics"
    
    # valid time units are "ns", "us" (or "Âµs"), "ms", "s", "m", "h"
    # required
    interval = "10s"
    
    ## Optional TLS Config
    tls_open = false
